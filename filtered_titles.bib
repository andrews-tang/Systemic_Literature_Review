 journal = {KSII Transactions on Internet and Information Systems},
 keywords = {Blockchain, Smart contract, Supply chains, Ethereum, Block-chain, Transparency, Internet of things, Food supply, Digital storage, Customer satisfaction, Confidential data, Data and information, Data leakage, Ethermint, Filesystem, Information use, Inter planetary file system, Lelantos, Smart chain smart contract, Tendermint, Water supply},
 note = {Type: Review},
 number = {11},
