 journal = {Scientific Reports},
 keywords = {Blockchain, blockchain, food safety, Food Safety, animal, Animals, article, breeding, Columbidae, consumer, environmental parameters, food industry, Food Industry, Hazard Analysis and Critical Control Points, hazard assessment, human, Humans, information storage, information system, nonhuman, poultry, slaughtering, *Blockchain, *Hazard Analysis and Critical Control Points},
 note = {Type: Article},
 number = {1},
 title = {Credible pigeon permissioned blockchain traceability platform integrated with {IoT} based on {HACCP}},
 url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-85144636642&doi=10.1038%2fs41598-022-27065-2&partnerID=40&md5=f587f1fb9cfbd2392961850cafd92551},
