 note = {Type: Article},
 title = {Blockchain-{Based} {Internet} of {Things}: {Machine} {Learning} {Tea} {Sensing} {Trusted} {Traceability} {System}},
 url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-85126762355&doi=10.1155%2f2022%2f8618230&partnerID=40&md5=6c9024950d3c7e9149e630c2ea221bd4},
