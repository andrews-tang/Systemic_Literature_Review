 month = {September},
 number = {5},
 pages = {91--97},
 title = {Private-{Blockchain}-{Based} {Industrial} {IoT} for {Material} and {Product} {Tracking} in {Smart} {Manufacturing}},
 volume = {34},
 year = {2020}
}

