 doi = {10.3390/agriculture12010040},
 journal = {Agriculture (Switzerland)},
 note = {Type: Review},
 number = {1},
 title = {Agriculture-{Food} {Supply} {Chain} {Management} {Based} on {Blockchain} and {IoT}: {A} {Narrative} on {Enterprise} {Blockchain} {Interoperability}},
 url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-85122799004&doi=10.3390%2fagriculture12010040&partnerID=40&md5=57c61f3bc664a95392ed8e4723f1319c},
