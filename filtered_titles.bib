 title = {Fusion of {Blockchain}-{IoT} network to improve supply chain traceability using {Ethermint} {Smart} chain: {A} {Review}},
 url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-85143538741&doi=10.3837%2ftiis.2022.11.013&partnerID=40&md5=e6b2b150318fadc05cb4f455906e99ec},
