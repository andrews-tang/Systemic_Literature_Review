 keywords = {Blockchain, Supply chains, Visibility, Internet of things, Supply chain visibility, Internet of thing (IOT), Tracking and tracing, Agile supply chains, Fixed period, Granular datum, Real-time data, Sensor networks},
 note = {Type: Article},
 number = {2},
