 doi = {https://doi.org/10.1016/j.jii.2022.100381},
 file = {Full Text:files/16463/Ferrández-Pastor et al. - 2022 - Agricultural traceability model based on IoT and B.pdf:application/pdf},
 issn = {2452-414X},
 journal = {Journal of Industrial Information Integration},
 keywords = {Blockchain, IoT, Traceability, Industrial hemp, Tamper proof tech},
 pages = {100381},
 title = {Agricultural traceability model based on {IoT} and {Blockchain}: {Application} in industrial hemp production},
 url = {https://www.sciencedirect.com/science/article/pii/S2452414X22000498},
